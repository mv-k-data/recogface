{% for item in items %}
    <div class="col" >
        <div class="card h-100 js-result-item" data-href="{{ item.image_url }}">
            <img src="{{ item.full_image_name }}" class="rounded mx-auto d-block" alt="..."> 
            <div class="card-body">
            <p class="card-text">{{ item.image_text }}</p>
            <span class="badge bg-secondary"
                style="background: {{ config.SEARCHES_ENGINES.get(item.search_engine, '').get('color', 'silver')}} !important;"
            >
                {{item.search_engine}}
            </span>
            </div>
        </div>
    </div>
{% endfor %}